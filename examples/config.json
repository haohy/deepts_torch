{
    "dataset": {
        "dir_root": "~/TSF/deepts/examples/data",
        "bike_hour": {
            "file_path": "bike_hour.csv",
            "csv_kwargs": {
                "index_col": 0
            },
            "sparse_col": {
                "season": 0,
                "holiday": 0,
                "workingday": 5,
                "weathersit": 0
            },
            "dense_col": [
                "weekday", "yr", "mnth", "hr", "temp", "atemp", "hum", "windspeed", "casual", "registered"
            ],
            "target": "cnt"
        }
    },

    "model": {
        "model_dir": "./model_dir",
        "Test": {
            "model_name": "LSTM",
            "hid_dim": 64,
            "lag": 7,
            "n_back": 10,
            "n_fore": 5,
            "batch_size": 32,
            "callbacks": {
                "tensorboard": {
                    "log_dir": "./logs"
                }
            }
        },
        "LSTM": {
            "model_name": "LSTM",
            "hid_dim": 64,
            "lag": 7,
            "n_back": 24,
            "n_fore": 24,
            "batch_size": 32,
            "callbacks": {
                "tensorboard": {
                    "log_dir": "~/TSF/deepts/examples/logs"
                }
            }
        },
        "TCAN": {
            "model_name": "TCAN",
            "channel_list" : [20, 30, 40],
            "kernel_size": 3,
            "num_sub_blocks": 2,
            "attn_dim": 10,
            "temp_attn": true,
            "visual": false,
            "en_res": true,
            "is_conv": true,
            "softmax_axis": 1,
            "num_sparse_feat": 5,
            "num_dense_feat": 3,
            "lag": 7,
            "n_back": 24,
            "n_fore": 24,
            "batch_size": 32,
            "callbacks": {
                "tensorboard": {
                    "log_dir": "~/TSF/deepts/examples/logs"
                }
            }
        },
        "DeepAR": {
            "hid_dim": 64,
            "lag": 7,
            "batch_size": 32,
            "window_size": 10
        }
    }
}